{
  "manifest_version": 2,

  "name": "Premise Time Tracker",
  "description": "Control the Premise Time Tracker Wordpress plugin right from your browser.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "icons": ["icon.png"],
    "default_popup": "index.html",
    "default_title": "Start tracking time!"
  },
  "permissions": [
    "storage",
    "*://*/*",
    "webRequest",
    "webRequestBlocking"
  ]
}
